#include <stdio.h>
#include <math.h>
int main() {
    float y,hour,minute;
    scanf_s("%f",&y);
    hour=y*180/M_PI/30;
    printf(" Chasovaya %d\n",(int)hour);
    int c_hour=(int)hour;
    minute=(hour-c_hour)*60;
    printf("Minuty %d\n",(int)minute);
    printf("Gradus %d",(int)(6*minute));
    return(0);
}
